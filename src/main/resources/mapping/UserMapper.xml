<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.myblog.mapper.UserMapper">
    <select id="getUserByName" resultType="com.example.myblog.dto.User">
        select * from user where name=#{username}
    </select>

    <insert id="insertUser" parameterType="com.example.myblog.dto.User">
        insert into user values (#{name},#{password})
    </insert>
</mapper>